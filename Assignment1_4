
#include <stdio.h>

typedef int BOOL;
#define TRUE
#define FALSE
void Check(int iNo)
{
    if((iNo % 5)==0)
    {
        return TRUE;
    }
    else
    {
        return FALSE;
    }
    
}

int main()
{
    int iValue  =0;
    BOOL bRet = FALSE;
    
    printf("Enter number");
    scanf("%d",&iValue);
    
    bRet = Check(iValue);
    
    if(bRet == TRUE)
    {
        printf("Dicisible by 5");
    }
    else
    {
        printf("Not Divisible by 5");
    }
    
    return 0;
}
